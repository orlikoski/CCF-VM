#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_skadi import connection, actions, triggers


Name = 'Skadi'
Vendor = 'komand'
Version = '1.0.0'
Description = 'Open source collection of tools for digital forensics'


class KomandSkadi(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name, 
                vendor=Vendor, 
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.DataprocessingMovefromaws())

        self.add_action(actions.DataprocessingMovetoaws())

        self.add_action(actions.DataprocessingMvlocal())

        self.add_action(actions.ElasticsearchDelete())

        self.add_action(actions.ElasticsearchList())

        self.add_action(actions.TimesketchCreateuser())

        self.add_action(actions.TimesketchDeleteuser())


def main():
    """Run plugin"""
    cli = komand.CLI(KomandSkadi())
    cli.run()


if __name__ == "__main__":
    main()
